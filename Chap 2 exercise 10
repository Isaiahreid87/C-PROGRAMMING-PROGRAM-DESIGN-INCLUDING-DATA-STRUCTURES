/* Isaiah Reid Chapter 2 Programming exercise 10 */

#include <iostream>

using namespace std;

int main()
{
    
    double num1, num2, num3, num4, num5;
    int average;
    
    cout <<"What are your numbers: ";
    cin >> num1;
    cin >> num2;
    cin >> num3;
    cin >> num4;
    cin >> num5;
    
    average = (num1 + num2 + num3 + num4 + num5) + .5;
    
    cout <<"This is your average: " << average;
    
    return 0;
}
    
